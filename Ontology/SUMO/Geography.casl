. instance(vSINKING, LandSubsidence) =>
      exists vLAND: U
          . instance(vLAND, LandArea)
            /\ exactlyLocated(vSINKING, vLAND)
            /\ patient(vSINKING, vLAND)
. instance(vSINKING, LandSubsidence)
  /\ patient(vSINKING, vLAND)
  /\ instance(vLAND, LandArea)

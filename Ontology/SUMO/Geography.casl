. instance(partyToAgreement, BinaryPredicate)
. domain(partyToAgreement, a_1, Agent)
. domain(partyToAgreement, a_2, Proposition)
%{partyToAgreement
"(&%partyToAgreement ?AGENT ?PROP) 
means that the &%Agent ?AGENT has committed to the agreement ?PROP."}%
. partyToAgreement(vAGENT, vCONTENT) =>
      instance(vCONTENT, Agreement)
. partyToAgreement(vAGENT, vCONTENT) =>
      exists vCOMMITTING: U; vCBO: U
          . instance(vCOMMITTING, Committing)
            /\ instance(vCBO, ContentBearingObject)
            /\ patient(vCOMMITTING, vCBO)
            /\ containsInformation(vCBO, vCONTENT)
            /\ agent(vCOMMITTING, vAGENT)

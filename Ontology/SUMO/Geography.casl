. domain(landAreaOnly, a_2, AreaMeasure)
%{landAreaOnly
"(&%landAreaOnly ?REGION ?MEASURE) means 
that the total area(s) of solid ground within the &%GeographicArea 
?REGION has the &%AreaMeasure ?AMOUNT. The pieces of solid ground need 
not be continuous within the region."}%
. instance(waterAreaOnly, BinaryPredicate)
. domain(waterAreaOnly, a_1, GeographicArea)
. domain(waterAreaOnly, a_2, AreaMeasure)
%{waterAreaOnly
"(&%waterAreaOnly ?REGION ?MEASURE) means 
that the total area(s) of surface water within the &%GeographicArea 
?REGION has the &%AreaMeasure ?AMOUNT. The pieces of water need not be 
continuous within the region."}%
. subclass(vUNIT, AreaMeasure)
  /\ landAreaOnly(vAREA, MeasureFn(vLAND, vUNIT))
  /\ landAreaOnly(vAREA, MeasureFn(vWATER, vUNIT)) =>
      totalArea(vAREA, MeasureFn(AdditionFn(vLAND, vWATER), vUNIT))
. subclass(SurfaceGroundArea, GeographicArea)
%{SurfaceGroundArea
"&%SurfaceGroundArea is a subclass of 
&%GeographicArea that is restricted to regions whose surface is solid 
ground.  A &%SurfaceGroundArea may be a discontinuous region overlapping 
a larger, continuous &%GeographicArea but excluding any &%WaterAreas 
enclosed therein. Rivers, lakes, reservoirs and other surface water areas 
are not part of any &%SurfaceGroundArea."}%
. instance(vEXCLUSIVELANDAREA, SurfaceGroundArea) =>
      not (exists vWATERAREA: U
               . instance(vWATERAREA, WaterArea)
                 /\ part(vWATERAREA, vEXCLUSIVELANDAREA))
. landAreaOnly(vAREA, vMEASURE) =>
      exists vLAND: U
          . instance(vLAND, SurfaceGroundArea)
            /\ part(vLAND, vAREA)
            /\ totalArea(vLAND, vMEASURE)
. subclass(WaterOnlyArea, GeographicArea)
%{WaterOnlyArea
"&%WaterOnlyArea is a subclass of 
&%GeographicArea that is restricted to regions whose surface is water.  
A &%WaterOnlyArea may be a discontinuous region overlapping a larger, 
continuous &%GeographicArea but excluding any &%LandAreas enclosed therein. 
Dry land areas, including islands, are not part of any &%WaterOnlyArea."}%
. instance(vEXCLUSIVEWATERAREA, WaterOnlyArea) =>
      not (exists vLANDAREA: U
               . instance(vLANDAREA, LandArea)
                 /\ part(vLANDAREA, vEXCLUSIVEWATERAREA))
. waterAreaOnly(vAREA, vMEASURE) =>
      exists vWATER: U
          . instance(vWATER, WaterOnlyArea)
            /\ part(vWATER, vAREA)
            /\ totalArea(vWATER, vMEASURE)
. instance(SquareKilometer, UnitOfMeasure)
. subclass(SquareKilometer, AreaMeasure)
%{SquareKilometer
"&%SquareKilometer represents a 
&%UnitOfMeasure equal to one square kilometer."}%
. MeasureFn(a_1, SquareKilometer) =
      MultiplicationFn(MeasureFn(a_1, KiloFn(Meter)),
                       MeasureFn(a_1, KiloFn(Meter)))
. instance(vNUM, RealNumber) =>
      MeasureFn(vNUM, SquareKilometer) =
          MeasureFn(MultiplicationFn(vNUM, a_1_0_0_0_0_0_0), SquareMeter)
. instance(comparativeArea, QuaternaryPredicate)
. domain(comparativeArea, a_1, GeographicArea)
. domain(comparativeArea, a_2,
         IntersectionFn(RelationExtendedToQuantities, BinaryPredicate))
. domain(comparativeArea, a_3, PositiveRealNumber)
. domain(comparativeArea, a_4, GeographicArea)
%{comparativeArea
"(&%comparativeArea ?REGION1 ?RELATION ?FACTOR ?REGION2) means that 
the total area of &%GeographicArea ?REGION1 has the relation ?RELATION 
to the total area of &%GeographicArea ?REGION2 times ?FACTOR.  For 
example, that France is (slightly) less than twice the size of Colorado 
is represented by (&%comparativeArea &%France &%lessThan 2 &%Colorado)."}%
. comparativeArea(vREGION1, vRELATION, vFACTOR, vREGION2)
  /\ subclass(vUNIT, AreaMeasure)
  /\ instance(vUNIT, UnitOfMeasure)
  /\ measure(vREGION1, MeasureFn(vNUM1, vUNIT))
  /\ measure(vREGION2, MeasureFn(vNUM2, vUNIT)) =>
      holds(vRELATION, MeasureFn(vNUM1, vUNIT),
            MeasureFn(MultiplicationFn(vFACTOR, vNUM2), vUNIT))
. instance(totalLandBoundary, BinaryPredicate)
. domain(totalLandBoundary, a_1, GeographicArea)
. domain(totalLandBoundary, a_2, LengthMeasure)
%{totalLandBoundary
"(&%totalLandBoundary ?REGION ?LENGTH) 
means that the &%GeographicArea ?REGION has a total &%LengthMeasure 
?LENGTH of land boundaries between it and other countries.  Note that 
the boundaries included in the total length may be discontinuous, as when 
interrupted by stretches of &%Seacoast."}%
. instance(BorderFn, BinaryFunction)
. names(a_Quot_b_o_r_d_e_r_Quot, BorderFn)
. domain(BorderFn, a_1, GeographicArea)
. domain(BorderFn, a_2, GeographicArea)
. range(BorderFn, GeographicArea)
%{BorderFn
"(&%BorderFn ?AREA1 ?AREA2) denotes the 
border area where the &%GeographicAreas ?AREA1 and ?AREA2 meet."}%
. instance(BorderFn(vAREA1, vAREA2), GeographicArea) =>
      meetsSpatially(vAREA1, vAREA2)
. instance(vOBJ1, GeographicArea)
  /\ instance(vOBJ2, GeographicArea) =>
      BorderFn(vOBJ1, vOBJ2) = BorderFn(vOBJ2, vOBJ1)
. instance(BorderFn(vCOUNTRY1, vCOUNTRY2), GeographicArea)
  /\ instance(vCOUNTRY1, Nation)
  /\ instance(vCOUNTRY2, Nation) =>
      instance(BorderFn(vCOUNTRY1, vCOUNTRY2), InternationalBorder)
. instance(BorderFn(vAREA1, vAREA2), GeographicArea) =>
      orientation(vAREA1, vAREA2, Adjacent)
. orientation(vONE, vTWO, Adjacent) =>
      orientation(vTWO, vONE, Adjacent)
. orientation(vONE, vTWO, Near) => orientation(vTWO, vONE, Near)
. meetsSpatially(vAREA1, vAREA2) =>
      not overlapsSpatially(vAREA1, vAREA2)
. subclass(InternationalBorder, GeographicArea)
%{InternationalBorder
"&%InternationalBorder is the 
subclass of &%GeographicAreas where the areas of two &%Nations meet."}%
. subclass(SymmetricAttribute, RelationalAttribute)
%{SymmetricAttribute
"&%SymmetricAttribute is the class of 
&%RelationalAttributes that hold between two items regardless of their 
order or orientation."}%
. instance(Near, SymmetricAttribute)
. instance(Adjacent, SymmetricAttribute)
. not instance(On, SymmetricAttribute)
. not instance(Right, SymmetricAttribute)
. not instance(Left, SymmetricAttribute)
. not instance(Below, SymmetricAttribute)
. not instance(Above, SymmetricAttribute)
. subclass(PerimeterArea, Region)
%{PerimeterArea
"A &%PerimeterArea is a region that 
extends outward from a boundary with another region, surrounding or 
partially surrounding it, but which is not part of that other region.  
See &%BorderFn and &%PerimeterAreaFn."}%
. instance(PerimeterAreaFn, UnaryFunction)
. domain(PerimeterAreaFn, a_1, Region)
. rangeSubclass(PerimeterAreaFn, PerimeterArea)
%{PerimeterAreaFn
"(&%PerimeterAreaFn ?REGION) denotes a 
class including any peripheral zones extending outward from the &%Region 
?REGION."}%
. instance(vzone, PerimeterAreaFn(vregion)) =>
      meetsSpatially(vzone, vregion)
. instance(MaritimeClaimsTerritorialSeaFn, UnaryFunction)
. domain(MaritimeClaimsTerritorialSeaFn, a_1, Region)
. range(MaritimeClaimsTerritorialSeaFn, PerimeterArea)
%{MaritimeClaimsTerritorialSeaFn
"(&%MaritimeClaimsTerritorialSeaFn ?REGION) denotes a 
peripheral zones of maritime control extending outward from the &%Region 
?REGION."}%
. instance(vregion, GeopoliticalArea) =>
      instance(MaritimeClaimsTerritorialSeaFn(vregion),
               PerimeterAreaFn(vregion))
. instance(vregion, GeopoliticalArea) =>
      instance(MaritimeClaimsTerritorialSeaFn(vregion),
               UniformPerimeterArea)
. instance(InnerBoundaryFn, UnaryFunction)
. domain(InnerBoundaryFn, a_1, Region)
. range(InnerBoundaryFn, a_2, Region)
%{InnerBoundaryFn
"(&%InnerBoundaryFn ?REGION) denotes the 
inner boundary of the &%Region ?REGION, where ?REGION has an inner and 
outer &%orientation with respect to another object."}%
. instance(vREGION, Region) =>
      superficialPart(InnerBoundaryFn(vREGION), vREGION)
. instance(OuterBoundaryFn, UnaryFunction)
. domain(OuterBoundaryFn, a_1, Region)
. range(OuterBoundaryFn, a_2, Region)
%{OuterBoundaryFn
"(&%OuterBoundaryFn ?REGION) denotes the 
outer boundary of the &%Region ?REGION, where ?REGION has an inner and 
outer &%orientation with respect to another object."}%
. instance(vREGION, Region) =>
      superficialPart(OuterBoundaryFn(vREGION), vREGION)
. subclass(UniformPerimeterArea, PerimeterArea)
%{UniformPerimeterArea
"A &%UniformPerimeterArea is one 
that has a defined uniform &%width."}%
. instance(vAREA, UniformPerimeterArea) =>
      exists vWIDTH: U
          . distance(InnerBoundaryFn(vAREA), OuterBoundaryFn(vAREA), vWIDTH)
. instance(vZONE, UniformPerimeterArea) =>
      exists vWIDTH: U
          . instance(vWIDTH, LengthMeasure) /\ width(vZONE, vWIDTH)
. instance(vZONE, UniformPerimeterArea)
  /\ part(vINNER, InnerBoundaryFn(vZONE)) =>
      exists vOUTER: U
          . part(vOUTER, OuterBoundaryFn(vZONE))
            /\ distance(vINNER, vOUTER, vWIDTH)
. instance(Outside, PositionalAttribute)
%{Outside
"&%Outside is a &%PositionalAttribute used to 
describe the relative location of one object or region to another 
region.  For example, (&%orientation &%Cuba &%UnitedStates &%Outside)."}%
. orientation(vOBJ, vREGION, Outside) => instance(vOBJECT, Region)
. orientation(vOBJ, vREGION, Outside) => instance(vREGION, Region)
. orientation(vOBJ, vREGION, Outside) =>
      not partlyLocated(vOBJ, vREGION)
. partlyLocated(vOBJ, vREGION) =>
      not orientation(vOBJ, vREGION, Outside)
. part(vOBJ, vREGION) => not orientation(vOBJ, vREGION, Outside)
. orientation(vOBJ, vREGION, Outside)
  /\ instance(vZONE, PerimeterAreaFn(vREGION))
  /\ instance(vZONE, UniformPerimeterArea)
  /\ width(vZONE, MeasureFn(vLIMIT, vUNIT))
  /\ distance(vOBJ, vREGION, MeasureFn(vFAR, vUNIT))
  /\ instance(vUNIT, UnitOfMeasure)
  /\ lessThanOrEqualTo(SubtractionFn(vFAR, vLIMIT), a_0) =>
      located(vOBJ, vZONE)
. orientation(vOBJ, vREGION, Outside)
  /\ instance(vZONE, PerimeterAreaFn(vREGION))
  /\ instance(vZONE, UniformPerimeterArea)
  /\ width(vZONE, MeasureFn(vLIMIT, vUNIT))
  /\ distance(vOBJ, vREGION, MeasureFn(vFAR, vUNIT))
  /\ instance(vUNIT, UnitOfMeasure)
  /\ greaterThan(SubtractionFn(vFAR, vLIMIT), a_0) =>
      not located(vOBJ, vZONE)
. instance(vAREA, MaritimeClaimsTerritorialSeaFn(vPLACE))
  /\ width(vAREA, MeasureFn(vWIDTH, vUNIT))
  /\ distance(vOBJ, vPLACE, MeasureFn(vDISTANCE, vUNIT))
  /\ lessThanOrEqualTo(vDISTANCE, vWIDTH) =>
      located(vOBJ, vAREA)
. instance(totalCoastline, BinaryPredicate)
. domain(totalCoastline, a_1, GeographicArea)
. domain(totalCoastline, a_2, LengthMeasure)
%{totalCoastline
"(&%totalCoastline ?AREA ?COASTLENGTH) 
means that the total length of all boundaries between the &%GeographicArea 
?AREA and the ocean is the &%LengthMeasure ?COASTLENGTH.  The coastline 
counted may be discontinuous and may even be along different oceans (as 
in &%Columbia).  A &%totalCoastline of zero indicates a &%LandlockedArea.  
Note that only &%Ocean coastlines are counted, not any shores with inland 
waters."}%
. subclass(LandlockedArea, LandArea)
%{LandlockedArea
"&%LandlockedArea is the class of 
&%LandAreas that lack access to an &%Ocean or to a &%Waterway 
providing a link to the ocean."}%
. instance(vAREA, LandlockedArea) =>
      not (exists vCOAST: U
               . instance(vCOAST, Seacoast) /\ part(vCOAST, vAREA))
. instance(vAREA, LandlockedArea)
  /\ subclass(vUNIT, LengthMeasure) =>
      totalCoastline(vAREA, MeasureFn(a_0, vUNIT))
. subclass(vUNIT, LengthMeasure)
  /\ totalCoastline(vAREA, MeasureFn(a_0, vUNIT)) =>
      instance(vAREA, LandlockedArea)
. instance(vAREA, LandlockedArea)
  /\ instance(vAREA, GeopoliticalArea) =>
      not (exists vCLAIM: U
               . instance(vCLAIM, MaritimeClaimArea)
                 /\ claimedTerritory(vCLAIM, vAREA))
. instance(vAREA, LandlockedArea) =>
      not (exists vWATER: U
               . instance(vWATER, Ocean) /\ meetsSpatially(vAREA, vWATER))
. instance(vAREA, LandlockedArea) =>
      not (exists vWATER: U
               . instance(vWATER, SaltWaterArea)
                 /\ part(vWATER, vSEA)
                 /\ instance(vSEA, Ocean)
                 /\ meetsSpatially(vAREA, vWATER))
. instance(vAREA, LandlockedArea) =>
      exists vLAND: U
          . instance(vLAND, LandArea) /\ meetsSpatially(vAREA, vLAND)
. instance(claimedTerritory, BinaryPredicate)
. domain(claimedTerritory, a_1, GeographicArea)
. domain(claimedTerritory, a_2, Agent)
. subrelation(dependentGeopoliticalArea, claimedTerritory)
%{claimedTerritory
"(&%claimedTerritory ?AREA ?POLITY) 
means that some right over the &%GeographicArea ?AREA is claimed by 
the &%Agent or &%GeopoliticalArea ?POLITY.  If two politically independent 
states or agents claim the same area, that area is a 'disputed 
territory'."}%
. instance(maritimeClaimType, BinaryPredicate)
. instance(maritimeClaimType, AsymmetricRelation)
. domain(maritimeClaimType, a_1, GeopoliticalArea)
. domainSubclass(maritimeClaimType, a_2, MaritimeClaimArea)
%{maritimeClaimType
"(&%maritimeClaimType ?POLITY ?TYPE) 
means that the &%GeopoliticalArea ?POLITY claims rights over a 
&%MaritimeClaimArea of ?TYPE."}%
. instance(vAREA, LandlockedArea)
  /\ subclass(vCLAIM, MaritimeClaimArea) =>
      not maritimeClaimType(vAREA, vCLAIM)
. maritimeClaimType(vAREA, vTYPE)
  /\ subclass(vTYPE, MaritimeClaimArea) =>
      exists vCLAIM: U
          . instance(vCLAIM, vTYPE) /\ claimedTerritory(vCLAIM, vAREA)
. instance(vAREA, vTYPE)
  /\ subclass(vTYPE, MaritimeClaimArea)
  /\ claimedTerritory(vAREA, vPOLITY) =>
      maritimeClaimType(vPOLITY, vTYPE)
. instance(waterDepth, BinaryPredicate)
. domain(waterDepth, a_1, WaterArea)
. domain(waterDepth, a_2, LengthMeasure)
. subrelation(waterDepth, length)
%{waterDepth
"(&%waterDepth ?AREA ?LENGTH) means that the 
depth of water at the &%Region ?AREA is ?LENGTH."}%
. waterDepth(vAREA, vLENGTH) => instance(vAREA, WaterArea)
. subclass(MaritimeClaimArea, GeographicArea)
%{MaritimeClaimArea
"A &%MaritimeClaimArea is a &%GeographicArea 
delimited by a geopolitical state's claim, under the &%LawOfTheSea, of rights 
to certain resources, activities, or jurisdiction in the claimed area.  
&%MaritimeClaimAreas can pertain to &%WaterAreas, &%SubmergedLandAreas, and &%Airspace."}%
. subclass(MaritimeContiguousZone, MaritimeClaimArea)
. subclass(MaritimeContiguousZone, SaltWaterArea)
%{MaritimeContiguousZone
"&%MaritimeContiguousZone 
is the subclass of &%MaritimeClaimArea that includes areas over 
which a geopolitical state may exercise some control of activities 
beyond the 12-mile zone of its &%TerritorialSea.  In general, 
the authorized control is for preventing or punishing activities 
that would violate laws applying within the 12-mile zone (that is, 
authorization to chase and intercept).  According to the &%LawOfTheSea, 
a &%MaritimeContiguousZone may extend up to 24 nautical miles from the coast."}%
. instance(MaritimeContiguousZoneFn, UnaryFunction)
. domain(MaritimeContiguousZoneFn, a_1, GeopoliticalArea)
. range(MaritimeContiguousZoneFn, MaritimeContiguousZone)
%{MaritimeContiguousZoneFn
"(&%MaritimeContiguousZoneFn ?POLITY) denotes the &%MaritimeContiguousZone 
that is claimed by the &%GeopoliticalArea ?POLITY."}%
. instance(vAREA, GeopoliticalArea)
  /\ instance(MaritimeContiguousZoneFn(vAREA),
              MaritimeContiguousZone) =>
      claimedTerritory(MaritimeContiguousZoneFn(vAREA), vAREA)
. subclass(MaritimeShelfArea, MaritimeClaimArea)
. subclass(MaritimeShelfArea, SubmergedLandArea)
%{MaritimeShelfArea
"&%MaritimeShelfArea 
is a subclass of &%MaritimeClaimArea that covers &%SubmergedLandAreas 
claimed by an adjacent geopolitical area.  The &%LawOfTheSea defines 
the continental shelf as extending up to 200 miles (NM) offshore and 
including the resources found therein.  The defined claim area does not 
correspond exactly to a geological &%ContinentalShelf, but if there is one, 
the two areas will overlap spatially."}%
. instance(vZONE, MaritimeShelfArea)
  /\ length(vZONE, MeasureFn(vWIDTH, NauticalMile)) =>
      lessThanOrEqualTo(vWIDTH, a_2_0_0)
. instance(vZONE, MaritimeShelfArea) /\ length(vZONE, vWIDTH) =>
      lessThanOrEqualTo(vWIDTH, MeasureFn(a_2_0_0, NauticalMile))
. instance(vzone, MaritimeShelfArea)
  /\ instance(vshelf, ContinentalShelf)
  /\ instance(vcountry, Nation)
  /\ meetsSpatially(vshelf, vcountry)
  /\ claimedTerritory(vzone, vnation) =>
      overlapsSpatially(vzone, vshelf)
. instance(MaritimeShelfAreaFn, UnaryFunction)
. domain(MaritimeShelfAreaFn, a_1, GeopoliticalArea)
. range(MaritimeShelfAreaFn, MaritimeShelfArea)
%{MaritimeShelfAreaFn
"(&%MaritimeShelfAreaFn ?POLITY) denotes the &%MaritimeShelfArea 
that is claimed by the &%GeopoliticalArea ?POLITY."}%
. instance(vAREA, GeopoliticalArea)
  /\ instance(MaritimeShelfAreaFn(vAREA), MaritimeShelfArea) =>
      claimedTerritory(MaritimeShelfAreaFn(vAREA), vAREA)
. subclass(MaritimeExclusiveEconomicZone, MaritimeClaimArea)
. subclass(MaritimeExclusiveEconomicZone, SaltWaterArea)
%{MaritimeExclusiveEconomicZone
"&%MaritimeExclusiveEconomicZone is the subclass of &%MaritimeClaimArea 
that represents the offshore area that coastal nations can claim for 
fishing and other uses of the ocean water and seabed found there."}%
. instance(vZONE, MaritimeExclusiveEconomicZone)
  /\ length(vZONE, MeasureFn(vWIDTH, NauticalMile)) =>
      lessThanOrEqualTo(vWIDTH, a_2_0_0)
. instance(vZONE, MaritimeExclusiveEconomicZone)
  /\ length(vZONE, vWIDTH) =>
      lessThanOrEqualTo(vWIDTH, MeasureFn(a_2_0_0, NauticalMile))
. instance(MaritimeExclusiveEconomicZoneFn, UnaryFunction)
. domain(MaritimeExclusiveEconomicZoneFn, a_1, GeopoliticalArea)
. range(MaritimeExclusiveEconomicZoneFn,
        MaritimeExclusiveEconomicZone)
%{MaritimeExclusiveEconomicZoneFn
"(&%MaritimeExclusiveEconomicZoneFn ?POLITY) denotes the 
&%MaritimeExclusiveEconomicZone that is claimed by the 
&%GeopoliticalArea ?POLITY."}%
. instance(vAREA, GeopoliticalArea)
  /\ instance(MaritimeExclusiveEconomicZoneFn(vAREA),
              MaritimeExclusiveEconomicZone) =>
      claimedTerritory(MaritimeExclusiveEconomicZoneFn(vAREA), vAREA)
. subclass(ExclusiveFishingZone, MaritimeClaimArea)
. subclass(ExclusiveFishingZone, SaltWaterArea)
%{ExclusiveFishingZone
"&%ExclusiveFishingZone 
is the subclass of &%MaritimeClaimArea including offshore areas over which 
a nation claims exclusive jurisdiction only for fishing purposes (cf. &%MaritimeExclusiveEconomicZone).  Zone widths vary up to 200 miles (NM)."}%
. instance(vZONE, ExclusiveFishingZone)
  /\ length(vZONE, MeasureFn(vWIDTH, NauticalMile)) =>
      lessThanOrEqualTo(vWIDTH, a_2_0_0)
. instance(vZONE, ExclusiveFishingZone) /\ length(vZONE, vWIDTH) =>
      lessThanOrEqualTo(vWIDTH, MeasureFn(a_2_0_0, NauticalMile))
. instance(ExclusiveFishingZoneFn, UnaryFunction)
. domain(ExclusiveFishingZoneFn, a_1, GeopoliticalArea)
. range(ExclusiveFishingZoneFn, ExclusiveFishingZone)
%{ExclusiveFishingZoneFn
"(&%ExclusiveFishingZoneFn ?POLITY) denotes the 
&%ExclusiveFishingZone that is claimed by the 
&%GeopoliticalArea ?POLITY."}%
. instance(vAREA, GeopoliticalArea)
  /\ instance(ExclusiveFishingZoneFn(vAREA), ExclusiveFishingZone) =>
      claimedTerritory(ExclusiveFishingZoneFn(vAREA), vAREA)
. subclass(ExtendedFishingZone, MaritimeClaimArea)
. subclass(ExtendedFishingZone, SaltWaterArea)
%{ExtendedFishingZone
"&%ExtendedFishingZone is the subclass of &%MaritimeClaimArea 
that includes offshore areas over which a nation claims fishing rights, 
beyond that nation's &%ExclusiveFishingZone.  Zone widths vary, 
from as little as 12 miles (NM) up to a width of 200 miles (NM)."}%
. instance(vZONE, ExtendedFishingZone)
  /\ length(vZONE, MeasureFn(vWIDTH, NauticalMile)) =>
      lessThanOrEqualTo(vWIDTH, a_2_0_0)
. instance(vZONE, ExtendedFishingZone) /\ length(vZONE, vWIDTH) =>
      lessThanOrEqualTo(vWIDTH, MeasureFn(a_2_0_0, NauticalMile))
. instance(ExtendedFishingZoneFn, UnaryFunction)
. domain(ExtendedFishingZoneFn, a_1, GeopoliticalArea)
. range(ExtendedFishingZoneFn, ExtendedFishingZone)
%{ExtendedFishingZoneFn
"(&%ExtendedFishingZoneFn ?POLITY) denotes the 
ExtendedFishingZoneFn that is claimed by the 
&%GeopoliticalArea ?POLITY, beyond its &%ExclusiveFishingZone."}%
. instance(vAREA, GeopoliticalArea)
  /\ instance(ExtendedFishingZoneFn(vAREA), ExtendedFishingZone) =>
      claimedTerritory(ExtendedFishingZoneFn(vAREA), vAREA)
. subclass(TerritorialSea, MaritimeClaimArea)
. subclass(TerritorialSea, SaltWaterArea)
%{TerritorialSea
"&%TerritorialSea is the class 
of contiguous waters over which a &%GeopoliticalArea claims 
jurisdiction in accordance with the United Nations Convention 
on the &%LawOfTheSea (LOS), Part II.  A territorial sea may be
up to 12 miles (&%NauticalMiles) in breadth.  A subclass of 
&%MaritimeClaimArea."}%
. instance(vZONE, MaritimeContiguousZone)
  /\ claimedTerritory(vZONE, vAREA)
  /\ instance(vWATER, TerritorialSea)
  /\ claimedTerritory(vWATER, vAREA) =>
      between(vAREA, vWATER, vZONE)
. instance(vZONE, TerritorialSea)
  /\ length(vZONE, MeasureFn(vWIDTH, NauticalMile)) =>
      lessThanOrEqualTo(vWIDTH, a_1_2)
. instance(vZONE, TerritorialSea) /\ length(vZONE, vWIDTH) =>
      lessThanOrEqualTo(vWIDTH, MeasureFn(a_1_2, NauticalMile))
. instance(TerritorialSeaFn, UnaryFunction)
. domain(TerritorialSeaFn, a_1, GeopoliticalArea)
. range(TerritorialSeaFn, TerritorialSea)
%{TerritorialSeaFn
"(&%TerritorialSeaFn ?POLITY) denotes 
the &%TerritorialSea that is claimed by the &%GeopoliticalArea ?POLITY."}%
. instance(vAREA, GeopoliticalArea)
  /\ instance(TerritorialSeaFn(vAREA), TerritorialSea) =>
      claimedTerritory(TerritorialSeaFn(vAREA), vAREA)
. instance(climateTypeInArea, BinaryPredicate)
. domain(climateTypeInArea, a_1, GeographicArea)
. domainSubclass(climateTypeInArea, a_2, ClimateZone)
%{climateTypeInArea
"(&%climateTypeInArea ?TYPE ?REGION) 
means that all or &%part of the &%GeographicArea ?REGION is of the 
&%ClimateZone ?TYPE."}%
. climateTypeInArea(vAREA, vTYPE) =>
      exists vREGION: U
          . instance(vREGION, GeographicArea)
            /\ instance(vREGION, vTYPE)
            /\ part(vREGION, vAREA)
. instance(vAREA, vTYPE) /\ subclass(vTYPE, ClimateZone) =>
      climateTypeInArea(vAREA, vTYPE)
. subclass(ClimateZone, GeographicArea)
%{ClimateZone
"&ClimateZone is a subclass of &%GeographicArea in which regions 
are classified according to their long-term weather conditions. 
The subclasses of &%ClimateZone are based on the Koeppen Climate 
Classification system.  In the Koeppen system, climate zones are 
distinguished based on temperatures and rainfall."}%
. subclass(TropicalClimateZone, ClimateZone)
%{TropicalClimateZone
"&%TropicalClimateZone is a  
subclass of &%ClimateZone in which the average temperature of the 
coldest month is greater than 18 degrees Celsius.  This is Class 
'A' in the Koeppen climate classification system."}%
. instance(vAREA, TropicalClimateZone)
  /\ subclass(vMO, Month)
  /\ instance(vAREA, GeographicArea)
  /\ averageTemperatureForPeriod(vAREA, vMO, vTEMP) =>
      greaterThan(vTEMP, MeasureFn(a_1_8, CelsiusDegree))
. instance(averageRainfallForPeriod, TernaryPredicate)
%{averageRainfallForPeriod
"(averageRainfallForPeriod ?AREA ?MO ?AMOUNT)
means that ?AREA receives ?AMOUNT of rain in month ?MO in an average
year.  Note that ?AMOUNT is a linear measure indicating the depth of water that
would accumulate over ?AREA if all water were captured."}%
. domain(averageRainfallForPeriod, a_1, GeographicArea)
. domain(averageRainfallForPeriod, a_2, Month)
. domain(averageRainfallForPeriod, a_3, LengthMeasure)
. subclass(WetTropicalClimateZone, TropicalClimateZone)
%{WetTropicalClimateZone
"&%WetTropicalClimateZone is 
the subclass of &%TropicalClimateZone that is characterized by having 
no dry months.  At least 60 mm of rainfall occur in the driest month. 
This is subtype 'Af' in the Koeppen climate system."}%
. instance(vAREA, TropicalClimateZone)

. instance(terrainInArea, BinaryPredicate)
. instance(terrainInArea, AsymmetricRelation)
. domain(terrainInArea, a_1, GeographicArea)
. domain(terrainInArea, a_2, TerrainAttribute)
%{terrainInArea
"(&%terrainInArea ?AREA ?ATTRIBUTE) 
means that the &%GeographicArea ?AREA is or includes a region with  
the features of &%TerrainArea ?ATTRIBUTE."}%
. terrainInArea(vAREA, vATTRIBUTE) =>
      exists vREGION: U
          . instance(vREGION, GeographicArea)
            /\ attribute(vREGION, vATTRIBUTE)
            /\ partlyLocated(vREGION, vAREA)
. instance(vAREA, GeographicArea)
  /\ attribute(vREGION, vATTRIBUTE)
  /\ instance(vATTRIBUTE, TerrainAttribute)
  /\ partlyLocated(vREGION, vAREA) =>
      terrainInArea(vAREA, vATTRIBUTE)
. subclass(TerrainAttribute, InternalAttribute)
%{TerrainAttribute
"&%TerrainAttribute is a class of 
&%Attributes that describe terrain."}%
. instance(vATT, TerrainAttribute) /\ attribute(vOBJECT, vATT) =>
      instance(vOBJECT, LandArea)
. instance(FlatTerrain, TerrainAttribute)
%{FlatTerrain
"&%FlatTerrain is a &%TerrainAttribute 
describing regions within which there is very little variation in 
&%altitude."}%
. attribute(vAREA, FlatTerrain)
  /\ part(vZONE, vAREA)
  /\ slopeGradient(vZONE, vSLOPE) =>
      greaterThan(a_0_Period_0_0_5, vSLOPE)
. instance(LowTerrain, TerrainAttribute)
%{LowTerrain
"&%LowTerrain is terrain in which the slope 
is less than 3%."}%
. attribute(vAREA, FlatTerrain)
  /\ part(vZONE, vAREA)
  /\ slopeGradient(vZONE, vSLOPE) =>
      greaterThan(a_0_Period_0_3, vSLOPE)
. instance(SteepTerrain, TerrainAttribute)
%{SteepTerrain
"A &%TerrainAttribute of a region in 
which there is a wide variation in &%elevation and &%slopeGradients 
of more than 10%."}%
. attribute(vAREA, SteepTerrain) =>
      exists vZONE: U
          . part(vZONE, vAREA)
            /\ slopeGradient(vZONE, vSLOPE)
            /\ greaterThan(vSLOPE, a_0_Period_1)
. instance(MountainousTerrain, TerrainAttribute)
. subAttribute(MountainousTerrain, SteepTerrain)
%{MountainousTerrain
"An area of &%MountainousTerrain 
is an area of rugged terrain in which there are many mountains."}%
. attribute(vAREA, MountainousTerrain) => instance(vAREA, LandArea)
. attribute(vAREA, MountainousTerrain) =>
      instance(vAREA, UplandArea)
. attribute(vAREA, MountainousTerrain) =>
      exists vMTN: U . instance(vMTN, Mountain) /\ part(vMTN, vAREA)
. attribute(vAREA, MountainousTerrain) =>
      exists vMTN: U . instance(vMTN, Mountain) /\ located(vMTN, vAREA)
. instance(vAREA, MountainRange) =>
      attribute(vAREA, MountainousTerrain)
. instance(FertileTerrain, TerrainAttribute)
%{FertileTerrain
"&%FertileTerrain describes an area 
that has the type of soil and climate conditions needed to produce 
good quality crops."}%
. attribute(vAREA, FertileTerrain) =>
      capability(Agriculture, located, vAREA)
. instance(vSOIL, Soil)
  /\ attribute(vSOIL, Yellow)
  /\ instance(vAREA, LandArea)
  /\ component(vSOIL, varea) =>
      not attribute(vAREA, FertileTerrain)
. instance(vSOIL, Soil)
  /\ attribute(vSOIL, Red)
  /\ instance(vAREA, LandArea)
  /\ component(vSOIL, vAREA) =>
      attribute(vAREA, FertileTerrain)
. instance(GeologicallyStable, TerrainAttribute)
%{GeologicallyStable
"&%GeologicallyStable is a 
&%TerrainAttribute of a &%GeographicArea in which the geological 
substructure is stable, i.e., there are no active &%Volcanoes, 
no major fault lines, no blasting or other destabilizing activity."}%
. instance(vAREA, GeographicArea)
  /\ instance(vCONE, Volcano)
  /\ attribute(vCONE, VolcanicallyActive) =>
      not attribute(vAREA, GeologicallyStable)
. instance(vAREA, GeographicArea)
  /\ instance(vBLOW, VolcanicEruption)
  /\ located(vBLOW, vAREA) =>
      not attribute(vAREA, GeologicallyStable)
. instance(vAREA, GeographicArea)
  /\ instance(vSHAKING, EarthTremor)
  /\ located(vSHAKING, vAREA) =>
      not attribute(vAREA, GeologicallyStable)
. instance(vAREA, GeographicArea)
  /\ instance(vFAULT, GeologicalFault)
  /\ located(vFAULT, vAREA) =>
      not attribute(vAREA, GeologicallyStable)
. instance(elevation, BinaryPredicate)
. domain(elevation, a_1, Object)
. domain(elevation, a_2, LengthMeasure)
%{elevation
"(&%elevation ?OBJECT ?HEIGHT) means that the 
physical &%Object ?OBJECT is located on the surface of &%PlanetEarth 
at the vertical distance ?HEIGHT above (or below, for a negative 
quantity) &%SeaLevel.  ?OBJECT may be a &%superficialPart of Earth's 
surface, such as a &%GeographicArea.  Elevation is measured from 
&%SeaLevel to the vertical top of the object."}%
. elevation(vOBJECT, vHEIGHT) =>
      exists vPLACE: U
          . instance(vPLACE, GeographicArea) /\ located(vOBJECT, vPLACE)
. elevation(vOBJECT, vHEIGHT)
  /\ located(vOBJECT, vPLACE)
  /\ instance(vPLACE, GeographicArea) =>
      superficialPart(vPLACE, PlanetEarth)
. elevation(vOBJECT, vHEIGHT) =>
      exists vDATUM: U
          . properPart(vDATUM, SeaLevel)
            /\ orientation(vOBJECT, vDATUM, Vertical)
            /\ distance(vOBJECT, vDATUM, vHEIGHT)
. elevation(vOBJECT, MeasureFn(vNUM, vUNIT))
  /\ instance(vUNIT, UnitOfMeasure)
  /\ instance(vOBJECT, LandForm) =>
      exists vHIGHPOINT: U
          . top(vHIGHPOINT, vOBJECT)
            /\ distance(vHIGHPOINT, SeaLevel, MeasureFn(vNUM, vUNIT))
. instance(ElevationLowPointFn, UnaryFunction)
. domain(ElevationLowPointFn, a_1, GeographicArea)
. range(ElevationLowPointFn, GeographicArea)
%{ElevationLowPointFn
"(&%ElevationLowPointFn ?AREA) 
denotes the area within the &%GeographicArea ?AREA that has the 
lowest &%elevation."}%
. instance(vAREA, GeographicArea)
  /\ geographicSubregion(ElevationLowPointFn(vAREA), vAREA)
  /\ elevation(ElevationLowPointFn(vAREA), vELEV1) =>
      not (exists vOTHER: U; vELEV2: U
               . geographicSubregion(vOTHER, vAREA)
                 /\ not vOTHER = ElevationLowPointFn(vAREA)
                 /\ elevation(vOTHER, vELEV2)
                 /\ lessThan(vELEV2, vELEV1))
. instance(ElevationHighPointFn, UnaryFunction)
. domain(ElevationHighPointFn, a_1, GeographicArea)
. range(ElevationHighPointFn, GeographicArea)
%{ElevationHighPointFn
"(&%ElevationHighPointFn ?AREA) 
denotes the area within the &%GeographicArea ?AREA that has the 
highest &%elevation."}%
. instance(vAREA, GeographicArea)
  /\ geographicSubregion(ElevationHighPointFn(vAREA), vAREA)
  /\ elevation(ElevationHighPointFn(vAREA), vELEV1) =>
      not (exists vOTHER: U; vELEV2: U
               . geographicSubregion(vOTHER, vAREA)
                 /\ not vOTHER = ElevationHighPointFn(vAREA)
                 /\ elevation(vOTHER, vELEV2)
                 /\ greaterThan(vELEV2, vELEV1))

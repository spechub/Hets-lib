  /\ instance(vWEATHER, PartlyCloudyWeather)
  /\ located(vWEATHER, vAREA) =>
      exists vFRACTION: U
          . cloudCoverFraction(vAREA, vFRACTION)
            /\ greaterThanOrEqualTo(vFRACTION, a_0_Period_3)
            /\ lessThanOrEqualTo(vFRACTION, a_0_Period_7)
. instance(vAREA, GeographicArea)
  /\ instance(vWEATHER, PartlyCloudyWeather)
  /\ located(vWEATHER, vAREA)
  /\ cloudCoverFraction(vAREA, vFRACTION) =>
      greaterThanOrEqualTo(vFRACTION, a_0_Period_3)
      /\ lessThanOrEqualTo(vFRACTION, a_0_Period_7)

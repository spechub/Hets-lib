  /\ instance(vQUAKE, Earthquake) =>
      exists vTREMOR: U
          . instance(vTREMOR, EarthTremor)
            /\ subProcess(vTREMOR, vQUAKE)
            /\ measure(vTREMOR, MeasureFn(vVALUE, RichterMagnitude))
